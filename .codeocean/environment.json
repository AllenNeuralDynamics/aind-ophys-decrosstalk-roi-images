{
	"version": 1,
	"base_image": "codeocean/jupyterlab:3.0.9-miniconda4.9.2-python3.8-ubuntu20.04",
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "python3-opencv",
					"version": "4.2.0+dfsg-5"
				},
				{
					"name": "vim",
					"version": "2:8.1.2269-1ubuntu5.29"
				}
			],
			"install_software_properties_common": true
		},
		"pip": {
			"packages": [
				{
					"name": "aind-data-schema",
					"version": "0.22.0"
				},
				{
					"name": "aind-ophys-utils",
					"version": "0.0.7"
				},
				{
					"name": "cellpose",
					"version": "2.2.3"
				},
				{
					"name": "h5py",
					"version": "3.10.0"
				},
				{
					"name": "matplotlib",
					"version": "3.7.3"
				},
				{
					"name": "numpy",
					"version": "1.24.4"
				},
				{
					"name": "opencv-python",
					"version": "4.9.0.80"
				},
				{
					"name": "pandas",
					"version": "2.0.3"
				},
				{
					"name": "pydantic",
					"version": "2.8.2"
				},
				{
					"name": "scikit-image",
					"version": "0.21.0"
				},
				{
					"name": "scipy",
					"version": "1.10.1"
				},
				{
					"name": "suite2p",
					"version": "0.14.3"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	},
	"env_variables": [
		{
			"name": "PIPELINE_URL",
			"value": "https://codeocean.allenneuraldynamics.org/capsule/6127754/tree"
		},
		{
			"name": "PIPELINE_VERSION",
			"value": "1.0"
		},
		{
			"name": "VERSION",
			"value": "5.0"
		}
	]
}